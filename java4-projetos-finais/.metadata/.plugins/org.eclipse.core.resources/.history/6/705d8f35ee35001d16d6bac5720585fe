package br.com.alura;

import java.util.ArrayList;

public class Testandolistas {

	public static void main(String[] args) {
		String aula1 = "1";
		String aula2 = "2";
		String aula3 = "3";
		
		ArrayList<String> aulas = new ArrayList<>();
		
		aulas.add(aula1);
		aulas.add(aula2);
		aulas.add(aula3);
		
		aulas.remove(0);
		
		
	}

}
